digraph finite_state_machine {
  rankdir=LR;
  overlap=scale
  splines=splines

  node [shape = circle, fixedsize=true, width=0.7];

  qAceita [shape=doublecircle]

  q0->qAceita [label="_ -> D"];
  q0->q1 [label="1 -> a, D"];
  q0->q4 [label="b -> D"];
  q4->qAceita [label="_ -> D"];
  q1->q2 [label="0 -> b, D"];
  q2->q3 [label="1 -> c, E"];
  q3->q0 [label="a -> D"];
  q4->q4 [label="b -> D\nc -> D"];
}
